                  "x": {
                    "type": "number",
                    "description": "x value"
                  },
                  "y": {
                    "type": "number",
                    "description": "y value"
                  },
                  "z": {
                    "type": "number",
                    "description": "z value"
                  },
                  "w": {
                    "type": "number",
                    "description": "w value"
                  }
                }
              }
            }
          },
          "shaft_length": {
            "type": "number",
            "description": "Length of the arrow shaft"
          },
          "shaft_diameter": {
            "type": "number",
            "description": "Diameter of the arrow shaft"
          },
          "head_length": {
            "type": "number",
            "description": "Length of the arrow head"
